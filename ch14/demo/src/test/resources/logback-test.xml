<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <include resource="org/springframework/boot/logging/logback/base.xml"/>
    <logger name="org.springframework" level="INFO"/>
    <logger name="com.example.demo" level="DEBUG"/>
    <logger name="org.springframework.web.servlet.resource.CachingResourceResolver" level="TRACE"/>
    <logger name="org.springframework.web.servlet.resource.EncodedResourceResolver" level="TRACE"/>
    <logger name="org.springframework.web.servlet.resource.EncodedResourceResolver" level="TRACE"/>
    <root level="INFO">
        <appender-ref ref="CONSOLE"/>
    </root>
</configuration>